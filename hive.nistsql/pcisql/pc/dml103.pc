  goto P160;
if (SQLCODE != 0)
  goto P130;
if (ctst1 == PID)
  ssflag = 1;
if (ctst1 < minpid)
  minpid = ctst1;
if (next == PID && ctst1 > PID)
  next = ctst1;
if (ctst1 > PID && ctst1 < next)
  next = ctst1;
printf ("Registered:  %ld\n", ctst1);
countr = countr + 1;
goto P132;

P160:
EXEC SQL CLOSE CYC6;
