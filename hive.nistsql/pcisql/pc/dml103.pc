/* First we delete any existing records.      */

EXEC SQL EXECUTE SYC1 USING :PID;
if (SQLCODE < 0)
  goto P103;

EXEC SQL EXECUTE SYC2 USING :testno, :PID;
if (SQLCODE < 0)
  goto P103;

