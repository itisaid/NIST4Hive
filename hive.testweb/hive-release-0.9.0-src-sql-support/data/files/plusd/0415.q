set hive.ql.mode=hql;
drop database IF EXISTS HU CASCADE;
create database HU;
use HU;
CREATE TABLE EXTREME_TEMPS(YEAR_OBSERV DOUBLE,HIGH DOUBLE,LOW DOUBLE) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' STORED AS TEXTFILE;
LOAD DATA LOCAL INPATH '_datapath_/plusd/0415/EXTREME_TEMPS.csv' OVERWRITE INTO TABLE EXTREME_TEMPS;
set hive.ql.mode=sql;
SELECT HIGH, YEAR_OBSERV, LOW FROM EXTREME_TEMPS ORDER BY YEAR_OBSERV DESC;
